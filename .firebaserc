{
  "projects": {
    "default": "die-partei-sh"
  },
  "hosting": {
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    }, {
      "source": "/**{,/**}",
      "destination": "/index.html"
    }, {
      "source": "!/@(js|css)/**",
      "destination": "/index.html"
    }]
  }
}
